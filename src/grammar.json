{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "webvtt",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "webvtt_block"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "items"
          }
        }
      ]
    },
    "items": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "style_block"
        },
        {
          "type": "SYMBOL",
          "name": "comment_block"
        },
        {
          "type": "SYMBOL",
          "name": "cue_block"
        },
        {
          "type": "SYMBOL",
          "name": "region_definition_block"
        }
      ]
    },
    "webvtt_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "byte_order_mark"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "WEBVTT"
        },
        {
          "type": "SYMBOL",
          "name": "line_terminator"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "line_terminator"
          }
        }
      ]
    },
    "region_definition_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "REGION"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "tabs_or_spaces"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "line_terminator"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "region_setting_component"
          }
        },
        {
          "type": "SYMBOL",
          "name": "line_terminator"
        }
      ]
    },
    "region_setting_component": {
      "type": "PATTERN",
      "value": "[^\\n\\r]+(\\n|\\r|\\r\\n)"
    },
    "comment_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "NOTE"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "tab_separator"
            },
            {
              "type": "SYMBOL",
              "name": "space_separator"
            },
            {
              "type": "SYMBOL",
              "name": "line_terminator"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "line_with_terminator"
          }
        },
        {
          "type": "SYMBOL",
          "name": "line_terminator"
        }
      ]
    },
    "style_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "STYLE"
        },
        {
          "type": "SYMBOL",
          "name": "line_terminator"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "line_with_terminator"
          }
        },
        {
          "type": "SYMBOL",
          "name": "line_terminator"
        }
      ]
    },
    "cue_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "cue_identifier"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "timestamp_range_line"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "cue_settings"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "line_terminator"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "line_with_terminator"
          }
        },
        {
          "type": "SYMBOL",
          "name": "line_terminator"
        }
      ]
    },
    "cue_settings": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "tabs_or_spaces"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "cue_setting"
          }
        }
      ]
    },
    "cue_setting": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "cue_setting_item"
        },
        {
          "type": "SYMBOL",
          "name": "separator_colon"
        },
        {
          "type": "SYMBOL",
          "name": "cue_setting_item"
        }
      ]
    },
    "timestamp_range_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "timestamp"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT1",
              "content": {
                "type": "SYMBOL",
                "name": "space_separator"
              }
            },
            {
              "type": "REPEAT1",
              "content": {
                "type": "SYMBOL",
                "name": "tab_separator"
              }
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "timestamp_arrow"
        },
        {
          "type": "SYMBOL",
          "name": "timestamp"
        }
      ]
    },
    "byte_order_mark": {
      "type": "PATTERN",
      "value": "(\\uEFBBBF|\\uFEFF|\\uFFFE)"
    },
    "webvtt_header": {
      "type": "PATTERN",
      "value": "WEBVTT"
    },
    "cue_identifier": {
      "type": "PATTERN",
      "value": "((((([^NSR\\n\\r][^\\n\\r]{5})|((N|S|R)[^OTE\\n\\r][^\\n\\r]{4})|((NO|ST|RE)[^TYG\\n\\r][^\\n\\r]{3})|((NOT|STY|REG)[^ELI\\n\\r][^\\n\\r]{2})|((STYL|REGI)[^EO\\n\\r][^\\n\\r])|(REGIO[^N\\n\\r])))((([^\\n\\r]{0,6}(\\n|\\r\\n?))|([^\\n\\r]{4}((-(([^-\\n\\r][^\\n\\r])|([^\\n\\r][^>\\n\\r]))[^\\n\\r]*)|([^\\n\\r]{1,5})|([^-\\n\\r][^\\r\\n]{2}((([^-\\n\\r][^\\n\\r]{2})|([^\\n\\r][^-\\n\\r][^\\n\\r])|([^\\n\\r][^\\n\\r][^>\\n\\r]))))[^\\n\\r]*)(\\n|\\r\\n?)))))|((([^\\n\\r]{3}[^E\\r\\n])|([^N\\r\\n][^\\n\\r]{3})|([^\\r\\n][^O\\r\\n][^\\n\\r]{2})|([^\\r\\n]{2}[^T\\r\\n][^\\n\\r])|([^\\n\\r]{4}[^E\\r\\n]))(\\n|\\r\\n?))|((([^S\\n\\r][^\\n\\r]{4})|([^\\n\\r][^T\\n\\r][^\\n\\r]{3})|([^\\n\\r]{2}[^Y\\n\\r][^\\n\\r]{2})|([^\\n\\r]{3}[^L\\n\\r][^\\n\\r]))(\\n|\\r\\n?))|([^\\n\\r]{1,3})(\\n|\\r\\n?))"
    },
    "cue_setting_item": {
      "type": "PATTERN",
      "value": "[^ :\\n\\r]+"
    },
    "separator_colon": {
      "type": "PATTERN",
      "value": ":"
    },
    "tab_separator": {
      "type": "PATTERN",
      "value": "\\t"
    },
    "space_separator": {
      "type": "PATTERN",
      "value": " "
    },
    "tabs_or_spaces": {
      "type": "PATTERN",
      "value": "[ \\t]+"
    },
    "line_terminator": {
      "type": "PATTERN",
      "value": "(\\n|\\r|\\n\\r)"
    },
    "timestamp": {
      "type": "PATTERN",
      "value": "([0-9]{2,}:)?([0-9]{2}:[0-9]{2}\\.[0-9]{3})"
    },
    "timestamp_range_arrow": {
      "type": "PATTERN",
      "value": " --> "
    },
    "timestamp_arrow": {
      "type": "PATTERN",
      "value": "-->"
    },
    "line_with_terminator": {
      "type": "PATTERN",
      "value": "((.{10}(([^-\\n][^\\n][^\\n])|([^\\n][^-\\n][^\\n])|([^\\n][^\\n][^>\\n]))[^\\n]*)|([^\\n]{1,12}))(\\n|\\r|\\n\\r|\\r\\n)"
    },
    "line_without_terminator": {
      "type": "PATTERN",
      "value": "((.{10}(([^-\\n][^\\n][^\\n])|([^\\n][^-\\n][^\\n])|([^\\n][^\\n][^>\\n]))[^\\n]*)|([^\\n]{1,12}))"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}