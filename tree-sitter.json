{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "webvtt",
      "camelcase": "WebVtt",
      "title": "WebVTT",
      "scope": "source.webvtt",
      "file-types": ["vtt"],
      "first-line-regex": "^WEBVTT(\n|\r\n?)",
      "injection-regex": "^webvtt$",
      "class-name": "TreeSitterWebvtt",
      "highlights": "queries/highlights.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A Tree-sitter grammar for WebVTT using Regex",
    "authors": [
      {
        "name": "Twaha Rahman",
        "email": "twaha.rahman.direct@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/Twaha-Rahman/tree-sitter-webvtt"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}

